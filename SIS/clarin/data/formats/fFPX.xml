<?xml-model href="../../schemas/format.xsd" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<format xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="fFPX" xsi:noNamespaceSchemaLocation="../../schemas/format.xsd" display="show">
  <titleStmt>
    <title>FlashPix</title>
    <abbr>FPX</abbr>
  </titleStmt>
  <keyword>raster image</keyword>
  <keyword>image data</keyword>
  <keyword>multi-resolution images</keyword>
   <info type="description">
   <p>
    FlashPix is a bitmapped computer graphics file format where the image is saved in more than one resolution.</p>
    <p>See:
      <a href="https://www.imaging.org/common/uploaded%20files/pdfs/Papers/1998/RP-0-69/2235.pdf"> description provided by the Society for Imaging Science and Technology</a>.</p>
