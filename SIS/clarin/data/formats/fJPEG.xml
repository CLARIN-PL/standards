<?xml-model href="../../schemas/format.xsd" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<format xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="fJPEG" xsi:noNamespaceSchemaLocation="../../schemas/format.xsd" display="show">
  <titleStmt>
    <title>Joint Photographic Experts Group</title>
