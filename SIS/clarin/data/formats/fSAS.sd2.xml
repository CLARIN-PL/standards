<?xml-model href="../../schemas/format.xsd" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<format xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="fSAS.sd2" xsi:noNamespaceSchemaLocation="../../schemas/format.xsd" display="show">
  <titleStmt>
    <title>Statistical Analysis System (SAS) Dataset File Format</title>
    <abbr>SAS.sd2</abbr>
  </titleStmt>  
  <keyword>statistical data</keyword>
  <keyword>SAS</keyword>
  <keyword>binary format</keyword>
