            <respStmt id="sARQL2">
                <resp>Editor</resp>
                <name>Eric Prud'hommeaux</name>
                <name>Andy Seaborne</name>
            </respStmt>
        </titleStmt>
        <versionNumber type="major">1.0</versionNumber>
        <date>2008-01-15</date>
        <info type="description">
            <p/>
        </info>
        <address type="URL">http://www.w3.org/TR/2008/REC-rdf-sparql-query-20080115/</address>
        <relation target="SpecRDF" type="isApplicationOf"/>
